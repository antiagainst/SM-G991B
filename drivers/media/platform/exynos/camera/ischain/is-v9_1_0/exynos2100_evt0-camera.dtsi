			"GATE_MIPI_PHY_LINK_WRAP_QCH_CSIS1",
			"GATE_MIPI_PHY_LINK_WRAP_QCH_CSIS2",
			"GATE_MIPI_PHY_LINK_WRAP_QCH_CSIS3",
			"GATE_MIPI_PHY_LINK_WRAP_QCH_CSIS4",
			"GATE_MIPI_PHY_LINK_WRAP_QCH_CSIS5",

			"GATE_PDP_TOP_QCH_PDP_TOP",
			"GATE_ADD_TAA_QCH",
			"GATE_ORBMCH_QCH",
			"GATE_LME_QCH",
			"GATE_MCFP0_QCH",
			"GATE_MCFP1_QCH",
			"GATE_DNS_QCH",
			"GATE_ITP_QCH",
			"GATE_YUVPP_TOP_QCH",
			"GATE_MCSC_QCH",
			"GATE_GDC0_QCH",
			"GATE_GDC1_QCH";
		status = "ok";
		iommus = <&sysmmu_d0_csis>,
		       <&sysmmu_d1_csis>,
		       <&sysmmu_d2_csis>,
		       <&sysmmu_d3_csis>,
		       <&sysmmu_d_lme>,
		       <&sysmmu_d_taa>,
		       <&sysmmu_d_yuvpp>,
		       <&sysmmu_d0_dns>,
		       <&sysmmu_d1_dns>,
		       <&sysmmu_d0_mcsc>,
		       <&sysmmu_d1_mcsc>,
		       <&sysmmu_d2_mcsc>,
		       <&sysmmu_d0_mcfp0>,
		       <&sysmmu_d1_mcfp0>,
		       <&sysmmu_d2_mcfp0>,
		       <&sysmmu_d3_mcfp0>,
		       <&sysmmu_d0_mcfp1>;
