				preprocessor {
					product_name = <100>; /* NOTHING */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};

				/* sensor modes */
				modes {
					/* common = <width, height, fps, settle, mode, lane, speed, interleave, lrte, pd_mode> */
					/* vcX = <map, hwformat, width, height, hwformat, type, width, height> */
					mode0 {
						option { votf = <1>; };
					};
					mode1 {
						option { votf = <1>; };
					};
					mode2 {
						option { votf = <1>; };
					};
					mode3 {
						option { votf = <1>; };
					};
					mode4 {
						option { votf = <1>; };
					};
					mode5 {
						option { votf = <1>; };
					};
					mode6 {
						option { votf = <1>; };
					};
					mode7 {
						option { votf = <1>; };
