&pinctrl_4 {
	/* PERIC0 USI15 REAR sub */
	/* No pull-up resistor, use internal one */
	hsi2c30_bus: hsi2c30-bus {
		samsung,pins = "gpp15-0", "gpp15-1";
		samsung,pin-function = <2>;
		samsung,pin-pud = <3>;
		samsung,pin-drv = <0>;
	};

	hsi2c30_bus_in: hsi2c30-bus-in {
		samsung,pins = "gpp15-0", "gpp15-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* PERIC0 USI15_I2C REAR main */
	hsi2c31_bus: hsi2c31-bus {
		samsung,pins = "gpp15-2", "gpp15-3";
		samsung,pin-function = <3>;
		samsung,pin-pud = <3>;
		samsung,pin-drv = <0>;
	};

	hsi2c31_bus_in: hsi2c31-bus-in {
		samsung,pins = "gpp15-2", "gpp15-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};
};

&pinctrl_5 {
	/* PERIC1 USI17_I2C front VT0 */
	/* No pull-up resistor, use internal one */
	hsi2c35_bus: hsi2c35-bus {
		samsung,pins = "gpp17-2", "gpp17-3";
		samsung,pin-function = <3>;
