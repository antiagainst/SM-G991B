#include <dt-bindings/camera/exynos_is_dt.h>

#include "camera/module_gm1sp.dtsi"
#include "camera/module_5e9.dtsi"
#include "camera/module_4h5yc.dtsi"

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;
			interrupt-parent = <&gic>;

			/* REAR CAMERA */
			is_sensor_gm1sp: is_sensor_gm1sp@43 {
				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk0_out>;
				pinctrl-2 = <&sensor_mclk0_fn>;
				pinctrl-3 = <>;

				position = <0>; /* Rear:0. Front:1 */
				id = <0>; /* sensor node id */
				mclk_ch = <0>;
				sensor_i2c_ch = <0>; /* I2C 0 */

				status = "okay";

				af {
					product_name = <23>; /* ACTUATOR_NAME_DW9839 */
					i2c_ch = <0>; /* SENSOR_CONTROL_I2C0 */
				};

				power_seq {
					gpio_mclk = <&gpg1 0 0x1>;

					scenario_normal_on {
						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "MCLK"; pin = <PIN_MCLK 1 0>; };
						20 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp18 3 0x1>; };
						30 { pname = "S2MPB02_LDO14"; pin = <PIN_REGULATOR 1 0>; };
						40 { pname = "S2MPB02_LDO13"; pin = <PIN_REGULATOR 1 0>; };
						50 { pname = "S2MPB02_LDO7"; pin = <PIN_REGULATOR 1 0>; };
						60 { pname = "S2MPB02_LDO10"; pin = <PIN_REGULATOR 1 0>; };
						61 { pname = "S2MPB02_LDO16"; pin = <PIN_REGULATOR 1 0>; };
						70 { pname = "S2MPB02_BUCK2"; pin = <PIN_REGULATOR 1 0>; };
						80 { pname = "on_i2c"; pin = <PIN_I2C 1 0>; };
						90 { pname = "pin"; pin = <PIN_FUNCTION 2 0>; };
						100 { pname = "sen_rst high"; pin = <PIN_OUTPUT 1 2000>; gpio = <&gpp18 3 0x1>; };
					};

					scenario_normal_off {
						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "sen_rst"; pin = <PIN_RESET 0 10>; gpio = <&gpp18 3 0x1>; };
						20 { pname = "sen_rst input"; pin = <PIN_INPUT 0 0>; gpio = <&gpp18 3 0x1>; };
						30 { pname = "S2MPB02_LDO14"; pin = <PIN_REGULATOR 0 0>; };
						40 { pname = "S2MPB02_LDO13"; pin = <PIN_REGULATOR 0 0>; };
						50 { pname = "S2MPB02_LDO7"; pin = <PIN_REGULATOR 0 0>; };
						60 { pname = "S2MPB02_LDO10"; pin = <PIN_REGULATOR 0 0>; };
						61 { pname = "S2MPB02_LDO16"; pin = <PIN_REGULATOR 0 0>; };
						70 { pname = "S2MPB02_BUCK2"; pin = <PIN_REGULATOR 0 0>; };
						80 { pname = "off_i2c"; pin = <PIN_I2C 0 0>; };
						90 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						100 { pname = "pin"; pin = <PIN_FUNCTION 1 0>; };
						110 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						120 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; };
					};

					scenario_vision_on {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_ON>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "MCLK"; pin = <PIN_MCLK 1 0>; };
						20 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp18 3 0x1>; };
						30 { pname = "S2MPB02_LDO14"; pin = <PIN_REGULATOR 1 0>; };
						40 { pname = "S2MPB02_LDO13"; pin = <PIN_REGULATOR 1 0>; };
						50 { pname = "S2MPB02_LDO7"; pin = <PIN_REGULATOR 1 0>; };
						60 { pname = "S2MPB02_LDO10"; pin = <PIN_REGULATOR 1 0>; };
						61 { pname = "S2MPB02_LDO16"; pin = <PIN_REGULATOR 1 0>; };
						70 { pname = "S2MPB02_BUCK2"; pin = <PIN_REGULATOR 1 0>; };
						80 { pname = "on_i2c"; pin = <PIN_I2C 1 0>; };
						90 { pname = "pin"; pin = <PIN_FUNCTION 2 0>; };
						100 { pname = "sen_rst high"; pin = <PIN_OUTPUT 1 2000>; gpio = <&gpp18 3 0x1>; };
					};

					scenario_vision_off {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_OFF>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "sen_rst"; pin = <PIN_RESET 0 10>; gpio = <&gpp18 3 0x1>; };
						20 { pname = "sen_rst input"; pin = <PIN_INPUT 0 0>; gpio = <&gpp18 3 0x1>; };
						30 { pname = "S2MPB02_LDO14"; pin = <PIN_REGULATOR 0 0>; };
						40 { pname = "S2MPB02_LDO13"; pin = <PIN_REGULATOR 0 0>; };
						50 { pname = "S2MPB02_LDO7"; pin = <PIN_REGULATOR 0 0>; };
						60 { pname = "S2MPB02_LDO10"; pin = <PIN_REGULATOR 0 0>; };
						61 { pname = "S2MPB02_LDO16"; pin = <PIN_REGULATOR 0 0>; };
						70 { pname = "S2MPB02_BUCK2"; pin = <PIN_REGULATOR 0 0>; };
						80 { pname = "off_i2c"; pin = <PIN_I2C 0 0>; };
						90 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						100 { pname = "pin"; pin = <PIN_FUNCTION 1 0>; };
						110 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						120 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; };
					};

				};
			};

			/* FRONT CAMERA */
			is_sensor_4h5yc: is_sensor_4h5yc@24 {
				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk4_out>;
				pinctrl-2 = <&sensor_mclk4_fn>;
				pinctrl-3 = <>;

				position = <1>; /* Rear:0. Front:1 */
				id = <1>; /* sensor node id */
				mclk_ch = <4>;
				sensor_i2c_ch = <1>;
				sensor_i2c_addr = <0x36>;

				status = "okay";

				power_seq {
					gpio_mclk = <&gpg2 1 0x1>;

					scenario_normal_on {
						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp9 0 0x1>; };
						20 { pname = "vdd_ldo18s"; pin = <PIN_REGULATOR 1 0>; };
						30 { pname = "vdd_ldo25"; pin = <PIN_REGULATOR 1 0>; };
						40 { pname = "S2MPB02_LDO4"; pin = <PIN_REGULATOR 1 0>; };
						50 { pname = "on_i2c"; pin = <PIN_I2C 1 0>; };
						60 { pname = "pin"; pin = <PIN_FUNCTION 2 0>; };
						70 { pname = "MCLK"; pin = <PIN_MCLK 1 0>; };
						80 { pname = "sen_rst high"; pin = <PIN_OUTPUT 1 0>; gpio = <&gpp9 0 0x1>; };
					};

					scenario_normal_off {
						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "vdd_ldo18s"; pin = <PIN_REGULATOR 0 0>; };
						20 { pname = "vdd_ldo25"; pin = <PIN_REGULATOR 0 0>; };
						30 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; };
						40 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						50 { pname = "sen_rst"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp9 0 0x1>; };
						60 { pname = "off_i2c"; pin = <PIN_I2C 0 0>; };
						70 { pname = "S2MPB02_LDO4"; pin = <PIN_REGULATOR 0 0>; };
					};

					scenario_vision_on {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_ON>;
						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp9 0 0x1>; };
						20 { pname = "vdd_ldo18s"; pin = <PIN_REGULATOR 1 0>; };
						30 { pname = "vdd_ldo25"; pin = <PIN_REGULATOR 1 0>; };
						40 { pname = "S2MPB02_LDO4"; pin = <PIN_REGULATOR 1 0>; };
						50 { pname = "on_i2c"; pin = <PIN_I2C 1 0>; };
						60 { pname = "pin"; pin = <PIN_FUNCTION 2 0>; };
						70 { pname = "MCLK"; pin = <PIN_MCLK 1 0>; };
						80 { pname = "sen_rst high"; pin = <PIN_OUTPUT 1 0>; gpio = <&gpp9 0 0x1>; };
					};

					scenario_vision_off {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_OFF>;
						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "vdd_ldo18s"; pin = <PIN_REGULATOR 0 0>; };
						20 { pname = "vdd_ldo25"; pin = <PIN_REGULATOR 0 0>; };
						30 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; };
						40 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						50 { pname = "sen_rst"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp9 0 0x1>; };
						60 { pname = "off_i2c"; pin = <PIN_I2C 0 0>; };
						70 { pname = "S2MPB02_LDO4"; pin = <PIN_REGULATOR 0 0>; };
					};

				};

			};

			/* Rear Sub CAMERA */
			is_sensor_5e9: is_sensor_5e9@41 {
				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk1_out>;
				pinctrl-2 = <&sensor_mclk1_fn>;
				pinctrl-3 = <>;

				/* board */
				position = <2>; /* Rear:0. Front:1, Tele:2 */
				id = <2>; /* fimc_is_sensor id */
				mclk_ch = <1>;
				sensor_i2c_ch = <2>; /* SENSOR_CONTROL_I2C2 */

				af {
					product_name = <100>; /* NOTHING */
				};

				flash {
					product_name = <100>; /* NOTHING */
				};

				preprocessor {
					product_name = <100>; /* NOTHING */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};

				power_seq {
					use = <1>;
					gpio_mclk = <&gpg1 1 0x1>;

					scenario_normal_on {
						sensor_scenario = <SENSOR_SCENARIO_NORMAL>;
						gpio_scenario = <GPIO_SCENARIO_ON>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "MCLK"; pin = <PIN_MCLK 1 0>; };
						20 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp8 0 0x1>; };
						30 { pname = "vdd_ldo21s"; pin = <PIN_REGULATOR 1 0>; };
						40 { pname = "vdd_ldo29"; pin = <PIN_REGULATOR 1 0>; };
						50 { pname = "S2MPB02_LDO3"; pin = <PIN_REGULATOR 1 0>; };
						60 { pname = "pin"; pin = <PIN_FUNCTION 2 1000>; };
						70 { pname = "sen_rst high"; pin = <PIN_OUTPUT 1 1000>; gpio = <&gpp8 0 0x1>; };
						80 { pname = "on_i2c"; pin = <PIN_I2C 1 0>; };
					};

					scenario_normal_off {
						sensor_scenario = <SENSOR_SCENARIO_NORMAL>;
						gpio_scenario = <GPIO_SCENARIO_OFF>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						20 { pname = "pin"; pin = <PIN_FUNCTION 1 0>; };
						30 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						40 { pname = "vdd_ldo21s"; pin = <PIN_REGULATOR 0 0>; };
						50 { pname = "S2MPB02_LDO3"; pin = <PIN_REGULATOR 0 0>; };
						60 { pname = "vdd_ldo29"; pin = <PIN_REGULATOR 0 0>; };
						70 { pname = "sen_rst"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp8 0 0x1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; };
						90 { pname = "off_i2c"; pin = <PIN_I2C 0 0>; };
					};

					scenario_vision_on {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_ON>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "MCLK"; pin = <PIN_MCLK 1 0>; };
						20 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp8 0 0x1>; };
						30 { pname = "vdd_ldo21s"; pin = <PIN_REGULATOR 1 0>; };
						40 { pname = "vdd_ldo29"; pin = <PIN_REGULATOR 1 0>; };
						50 { pname = "S2MPB02_LDO3"; pin = <PIN_REGULATOR 1 0>; };
						60 { pname = "pin"; pin = <PIN_FUNCTION 2 1000>; };
						70 { pname = "sen_rst high"; pin = <PIN_OUTPUT 1 1000>; gpio = <&gpp8 0 0x1>; };
						80 { pname = "on_i2c"; pin = <PIN_I2C 1 0>; };
					};

					scenario_vision_off {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_OFF>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						20 { pname = "pin"; pin = <PIN_FUNCTION 1 0>; };
						30 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						40 { pname = "vdd_ldo21s"; pin = <PIN_REGULATOR 0 0>; };
						50 { pname = "S2MPB02_LDO3"; pin = <PIN_REGULATOR 0 0>; };
						60 { pname = "vdd_ldo29"; pin = <PIN_REGULATOR 0 0>; };
						70 { pname = "sen_rst"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp8 0 0x1>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; };
						90 { pname = "off_i2c"; pin = <PIN_I2C 0 0>; };
					};
				};
			};

			is_sensor0: is_sensor0@17030000 {
				#address-cells = <1>;
				#size-cells = <1>;
				scenario = <SENSOR_SCENARIO_NORMAL>;	/* Normal, Vision, OIS etc */
				id = <0>;
				csi_ch = <0>;
				dma_ch = <0 0 0 0>;
				vc_ch = <0 1 2 3>;
				flite_ch = <FLITE_ID_NOTHING>;
				is_bns = <0>;
				status = "okay";
			};

			is_sensor1: is_sensor1@17040000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x17070000 0x1000>, /* MIPI-CSI2 */
					<0x0 0x170A0000 0x10000>; /* PHY: M0S4S4S4S4S4S2 */
				phys = <&mipi_phy_csis4_m0s4s4s4s4s4s2 0>;
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <1>;
				csi_ch = <4>;
				dma_ch = <1 1 1 1>;
				vc_ch = <0 1 2 3>;
				flite_ch = <FLITE_ID_NOTHING>;
				is_bns = <0>;
				status = "okay";

				interrupts = <0 INTREQ__CSIS4 IRQ_TYPE_LEVEL_HIGH>, /* MIPI-CSI2 */
					<0 INTREQ__CSIS_DMA1 IRQ_TYPE_LEVEL_HIGH>,
					<0 INTREQ__CSIS_DMA1 IRQ_TYPE_LEVEL_HIGH>,
					<0 INTREQ__CSIS_DMA1 IRQ_TYPE_LEVEL_HIGH>,
					<0 INTREQ__CSIS_DMA1 IRQ_TYPE_LEVEL_HIGH>;
			};

			is_sensor2: is_sensor2@17050000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x17040000 0x1000>, /* MIPI-CSI1 */
					<0x0 0x170A0000 0x10000>; /* PHY: M0S4S4S4S4S4S2 */
				phys = <&mipi_phy_csis1_m0s4s4s4s4s4s2 0>;
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <2>;
				csi_ch = <1>;
				dma_ch = <2 2 2 2>;
				vc_ch = <0 1 2 3>;
				flite_ch = <FLITE_ID_NOTHING>;
				is_bns = <0>;
				status = "okay";

				interrupts = <0 INTREQ__CSIS1 IRQ_TYPE_LEVEL_HIGH>, /* MIPI-CSI1 */
					<0 INTREQ__CSIS_DMA2 IRQ_TYPE_LEVEL_HIGH>,
					<0 INTREQ__CSIS_DMA2 IRQ_TYPE_LEVEL_HIGH>,
					<0 INTREQ__CSIS_DMA2 IRQ_TYPE_LEVEL_HIGH>,
					<0 INTREQ__CSIS_DMA2 IRQ_TYPE_LEVEL_HIGH>;
			};

			is_sensor3: is_sensor3@17060000 {
				#address-cells = <1>;
				#size-cells = <1>;
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <3>;
				csi_ch = <4>;
				dma_ch = <3 3 3 3>;
				vc_ch = <0 1 2 3>;
				flite_ch = <FLITE_ID_NOTHING>;
				is_bns = <0>;
				status = "disabled";
			};

			is_sensor4: is_sensor4@17070000 {
				#address-cells = <1>;
				#size-cells = <1>;
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <4>;
				csi_ch = <1>;
				dma_ch = <1 1 1 1>;
				vc_ch = <0 1 2 3>;
				flite_ch = <FLITE_ID_NOTHING>;
				is_bns = <0>;
				status = "disabled";
			};

			is_sensor5: is_sensor5@17080000 {
				#address-cells = <1>;
				#size-cells = <1>;
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <1>;
				csi_ch = <1>;
				dma_ch = <1 1 1 1>;
				vc_ch = <0 1 2 3>;
				flite_ch = <FLITE_ID_NOTHING>;
				is_bns = <0>;
				status = "disabled";
			};
		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

/***** reference symbol node *****/

&exynos_is {
	vender {
		rear_sensor_id = <43>; /* GM1SP */
		front_sensor_id = <24>; /* 4H5YC */
		rear2_sensor_id = <41>; /* 5E9 */
	};

	lic_offsets {
		/* 3AA = <setA lic offset0, setA lic offset1, lic offset2 set A trigger context */
		/* 	setB lic offset0, setB lic offset1, setB lic offset2, set B trigger context> */
		3AA = <5461 10922 16383 0 5461 10922 16383 0>;
	};

	is_dvfs {
		#define DVFS_INT_CAM_L0 690000
		#define DVFS_INT_CAM_L1 680000
		#define DVFS_INT_CAM_L2 670000
		#define DVFS_INT_CAM_L3 660000
		#define DVFS_INT_CAM_L4 650000
		#define DVFS_INT_CAM_L5 640000
		#define DVFS_INT_CAM_L6 630000

		#define DVFS_INT_L0 800000
		#define DVFS_INT_L1 533000
		#define DVFS_INT_L2 400000
		#define DVFS_INT_L3 267000
		#define DVFS_INT_L4 80000

		#define DVFS_CSIS_L0 690000
		#define DVFS_CSIS_L1 680000
		#define DVFS_CSIS_L2 670000
		#define DVFS_CSIS_L3 660000
		#define DVFS_CSIS_L4 650000
		#define DVFS_CSIS_L5 640000
		#define DVFS_CSIS_L6 630000

		#define DVFS_CAM_L0 690000
		#define DVFS_CAM_L1 680000
		#define DVFS_CAM_L2 670000
		#define DVFS_CAM_L3 660000
		#define DVFS_CAM_L4 650000
		#define DVFS_CAM_L5 640000
		#define DVFS_CAM_L6 630000

		#define DVFS_ISP_L0 690000
		#define DVFS_ISP_L1 680000
		#define DVFS_ISP_L2 670000
		#define DVFS_ISP_L3 660000
		#define DVFS_ISP_L4 650000
		#define DVFS_ISP_L5 640000
		#define DVFS_ISP_L6 630000

		#define DVFS_MIF_L0 3172000
		#define DVFS_MIF_L1 2730000
		#define DVFS_MIF_L2 2535000
		#define DVFS_MIF_L3 2288000
		#define DVFS_MIF_L4 2028000
		#define DVFS_MIF_L5 1716000
		#define DVFS_MIF_L6 1539000
		#define DVFS_MIF_L7 1352000
		#define DVFS_MIF_L8 1014000
		#define DVFS_MIF_L9 845000
		#define DVFS_MIF_L10 546000
		#define DVFS_MIF_L11 421000

		table0 {
			desc = "dvfs table v0.2";

			default_int_cam = <DVFS_INT_CAM_L0>;
			default_cam = <DVFS_CAM_L0>;
			default_csis = <DVFS_CSIS_L0>;
			default_isp = <DVFS_ISP_L0>;
			default_mif = <DVFS_MIF_L0>;
			default_int = <DVFS_INT_L0>;
