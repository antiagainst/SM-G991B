						//10 { pname = "ois_rst low"; pin = <PIN_OUTPUT 0 0>; };//gpio = <&gpp9 3 0x1>; ;
						10 { pname = "ois_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp9 3 0x1>; };
					};

					scenario_read_rom_on {
					};

					scenario_read_rom_off {
					};
				};
			};

