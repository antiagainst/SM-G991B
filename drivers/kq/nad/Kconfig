comment "Samsung Correlation information"
config SEC_KQ_CORRELATION_RESULT
	tristate "Samsung ASVG-VDDCAL Correlation"
	depends on SEC_KQ_NAD
	default n
	help
	  Samsung ASVG-VDDCAL Correlation information
	  checking relationship between base and real voltage
	  This feature only working exynos2100
	  This feature depends on SEC_KQ_NAD

comment "Samsung BPS Classifier information"
config SEC_KQ_BPS_RESULT
	tristate "Samsung BPS classifier result"
	depends on SEC_KQ_NAD
	default n
	help
	  Samsung BPS Classifier information
	  add BPS result to nad result
	  This feature works factory binary
	  This feature depends on SEC_KQ_NAD

config SEC_KQ_NAD_TEST
	bool "KUnit test for NAD"
	depends on KUNIT && SEC_KQ_NAD
	help
	  KQ NAD Unit Test for improve stability of code
	  KQ NAD Unit Test under User Mode Linux
	  Support 8 Test Cases

config SEC_KQ_NAD_API
	bool "Samsung NAD API"
	depends on SEC_KQ_NAD
	default n
	help
	  Samsung NAD API Feature, Enable NAD_API_TEST
	  checking whether pmic is normal or abnormal
	  This feature works factory binary
	  This feature depends on SEC_KQ_NAD
